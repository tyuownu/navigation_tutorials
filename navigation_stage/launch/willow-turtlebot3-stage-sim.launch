<launch>
  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>
  <include file="$(find navigation_stage)/move_base_config/move_base.xml" />
  <node pkg="stage_ros" type="stageros" name="stageros" args="$(find navigation_stage)/stage_config/worlds/willow-turtlebot3-5cm.world" respawn="false" >
    <param name="base_watchdog_timeout" value="0.2"/>
    <remap from="base_scan" to="/scan" />
  </node>
  <!--
  <include file="$(find navigation_stage)/move_base_config/slam_gmapping.xml"/>  
  -->
  <!--
  <include file="$(find navigation_stage)/launch/icpslam/nav2d_icp.launch" />
  -->
  <node name="cartographer_node" pkg="cartographer_ros" type="cartographer_node" args="
    -configuration_directory $(find cartographer_turtlebot)/configuration_files
    -configuration_basename turtlebot_urg_lidar_2d.lua" output="screen"></node>

  <node name="rviz" pkg="rviz" type="rviz" required="true" args="-d $(find cartographer_turtlebot)/configuration_files/demo_turtlebot.rviz"></node>
  <!--
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find navigation_stage)/rviz/navigation.rviz" />
  -->
</launch>
